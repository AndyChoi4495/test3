<div class='row'>
  <div class='col-md-12'>
    <h2>Student List</h2>
    <hr />
    <div class='table'>
      <thead>
        <tr>
          <th>Student Id</th>
          <th>Name</th>
          <th>Program</th>
          <th>GPA</th>
        </tr>
      </thead>
      <tbody>{{#each student}}
          <tr>
            <td>{{studId}}</td>
            <td>{{name}}</td>
            <td>{{program}}</td>
            <td>{{gpa}}</td>
          </tr>
        {{/each}}
      </tbody>
    </div>
  </div>
</div>